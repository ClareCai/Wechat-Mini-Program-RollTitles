<!--pages/play.wxml-->
<view class="play-container" style="background-color:{{playParams.backgroundColor}};" bindtap="onViewClick">
  <view
    class="play-text"
    style="font-size: {{playParams.fontSize * 4}}rpx;color:{{playParams.fontColor}};"
  >
    {{playParams.rollTitles}}
  </view>
  <t-button wx:if="{{showBtn}}" size="large" variant="outline" class="back-btn" bind:tap="onBackClick">返回</t-button>
</view>
